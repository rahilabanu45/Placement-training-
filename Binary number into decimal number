#include <stdio.h>
#include <math.h>

int main() {
    long binary;
    int decimal = 0, base = 1, rem;

    scanf("%ld", &binary);

    while (binary > 0) {
        rem = binary % 10;
        decimal = decimal + rem * base;
        binary = binary / 10;
        base = base * 2;
    }

    printf("Decimal value = %d", decimal);
    return 0;
}
